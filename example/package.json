    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.0",
    "prettier": "^3.2.0",
    "sass": "^1.90.0",
    "typescript": "^5.9.2",
    "vercel": "^33.5.1",
    "vite": "^5.0.0"
  },
  "pnpm": {
    "overrides": {
      "smooth-pinch-zoom": "link:.."
