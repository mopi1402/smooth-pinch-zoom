{
  "name": "smooth-pinch-zoom",
  "version": "1.1.0",
  "description": "Transform pinch-to-zoom gestures into smooth, precise page-level zoom with continuous granularity",
  "type": "module",
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "src/scss"
  ],
  "scripts": {
    "build": "rollup -c",
    "build:prod": "NODE_ENV=production rollup -c",
    "dev": "rollup -c --watch",
    "prepublishOnly": "npm run build:prod"
  },
  "keywords": [
    "zoom",
    "pinch",
    "touch",
    "gesture",
    "responsive",
    "accessibility",
    "browser"
  ],
  "author": "Pierre Moati",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/mopi1402/smooth-pinch-zoom.git"
  },
  "devDependencies": {
    "@rollup/plugin-terser": "^0.4.4",
    "@rollup/plugin-typescript": "^11.1.5",
    "rollup": "^4.9.6",
    "rollup-plugin-dts": "^6.2.3",
    "tslib": "^2.6.2",
    "typescript": "^5.3.3"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ]
}
