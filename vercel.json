{
  "version": 2,
  "builds": [
    {
      "src": "example/**/*",
      "use": "@vercel/static"
    },
    {
      "src": "dist/**/*",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/dist/(.*)",
      "dest": "/dist/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/example/$1"
    }
  ],
  "outputDirectory": "example",
  "public": true
}
